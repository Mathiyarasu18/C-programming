
#include<stdio.h>
#include<stdint.h>

struct carModel
{

	uint32_t carNumber;
	uint32_t carPrice;
	uint32_t carMaxspeed;
	float carWeight;
};

int main()
{
	struct carModel BMW = {8035,150000,300,500.65};
	struct carModel Ford = {.carPrice=100000,.carNumber=1234,.carMaxspeed=250,.carWeight=450.29};

	printf("Details of the BMW\n");
	printf("Car Number:%d\n",BMW.carNumber);
	printf("Car Price:%d\n",BMW.carPrice);
	printf("Car Maximum speed:%d\n",BMW.carMaxspeed);
	printf("Car Weight:%f\n",BMW.carWeight);


	printf("Details of FORD\n");
	printf("Car Number:%d\n",Ford.carNumber);
	printf("Car Price:%d\n",Ford.carPrice);
	printf("Car Maximum speed:%d\n",Ford.carMaxspeed);
	printf("Car Weight:%f\n",Ford.carWeight);

	return 0;
}
